<div class="<%= styleguide_class %>">
  <div>
    <h2>On hover</h2>
    <tooltip class="example-tooltip" text="Don't forget to use vue2-touch-events for this tooltip!">
      <div class="example-tooltip__icon"></div>
    </tooltip>
  </div>
  
  <div>
    <h2>On click</h2>
    <tooltip class="example-tooltip" text="Don't forget to use vue2-touch-events for this tooltip!" :on-hover="false">
      <div class="example-tooltip__icon"></div>
    </tooltip>
  </div>
</div>
